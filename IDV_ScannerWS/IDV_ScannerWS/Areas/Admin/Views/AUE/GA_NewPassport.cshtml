                                <label for="Txt_FistName">Detected Document ID</label>
                                <input type="text" class="form-control" id="Txt_DocumentIDF" placeholder="Enter Document ID">
                            </div>
                            <div class="form-group col-lg-6">
                                <label for="Txt_CounterID">Uploaded By [ Customer ]</label>
                                <select class="form-control form-control" id="Txt_CounterID">
                                    @if (ViewBag.DT != null)
                                    {
                                        int SelC = 0;
                                        foreach (DataRow Row in ViewBag.DT)
                                        {
                                            if (SelC == 0)
                                            {
                                                SelC = 1;
                                                <option value="@Row[4].ToString().Trim()" selected>@Row[1].ToString().Trim() - @Row[3].ToString().Trim() - @Row[5].ToString().Trim()</option>
                                            }
                                            else
                                            {
                                                <option value="@Row[4].ToString().Trim()">@Row[1].ToString().Trim() - @Row[3].ToString().Trim() - @Row[5].ToString().Trim()</option>
                                            }
                                        }
                                    }
                                </select>
                            </div>
                            <div class="form-group col-lg-6">
                                <label for="Txt_Status">Status</label>
                                <select class="form-control form-control" id="Txt_Status">
                                    <option value="1" selected>Accepted</option>
                                    <option value="2">Failed</option>
                                    <option value="3">Reffered</option>
                                    <option value="4">No Validation</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

