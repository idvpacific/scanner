@section JavaSCR{
    <script>
        function ShowReportList(PageCode, Unread) {
            var BaseUrl = "";
            switch (PageCode) {
                case '1': { BaseUrl = "@Url.Action("Passed", "ReportList")"; break; }
                case '2': { BaseUrl = "@Url.Action("Failed", "ReportList")"; break; }
                case '3': { BaseUrl = "@Url.Action("OCR", "ReportList")"; break; }
                case '4': { BaseUrl = "@Url.Action("Caution", "ReportList")"; break; }
                case '5': { BaseUrl = "@Url.Action("Attention", "ReportList")"; break; }
                case '6': { BaseUrl = "@Url.Action("Unknown", "ReportList")"; break; }
                case '7': { BaseUrl = "@Url.Action("Pending", "ReportList")"; break; }
                case '8': { BaseUrl = "@Url.Action("Waiting", "ReportList")"; break; }
                case '9': { BaseUrl = "@Url.Action("AllDocuments", "ReportList")"; break; }
            }
            if (Unread == '1') { BaseUrl = BaseUrl + "/UnRead"; }
            window.location.href = BaseUrl;
        }
    </script>
